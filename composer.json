{
    "name": "aldrumo/theme-manager",
    "description": "Laravel based Theme manager for your application",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "aldrumo/theme-installer": "~1.0",
        "illuminate/support": "^7.21",
        "aldrumo/support": "~0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Aldrumo\\ThemeManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aldrumo\\ThemeManager\\Tests\\": "tests/"
        }
    }
}
